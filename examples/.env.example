# Example .env file for Veil
# Copy this to .env and fill in your values

# API Configuration
API_KEY=your-api-key-here
API_URL=https://api.example.com

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=myapp_dev
DB_USER=postgres
DB_PASSWORD=secret

# Application Configuration
PORT=8080
DEBUG_MODE=true

# Feature Flags
ENABLE_CACHE=true
ENABLE_ANALYTICS=false
